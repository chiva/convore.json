[{"user_id": 1752, "stars": [], "topic_id": 13776, "date_created": 1300456175.7268, "message": "Since yesterday was St. Patrick's Day, there could be a few of you out there that are hungover today. So today's QOTD is to share your best hangover cures.", "group_id": 1105, "id": 382021}, {"user_id": 1752, "stars": [{"date_created": 1300462277.6438611, "user_id": 4095}, {"date_created": 1300469422.784734, "user_id": 3417}], "topic_id": 13776, "date_created": 1300456226.402004, "message": "Mine is a greasy diner breakfast (eggs, bacon, toast and breakfast potatoes) with black coffee and water.", "group_id": 1105, "id": 382033}, {"user_id": 6511, "stars": [{"date_created": 1300469420.202261, "user_id": 3417}], "topic_id": 13776, "date_created": 1300456684.6088359, "message": "I generally go with sleep and water... lots of water. That being said, greasy diner breakfasts are a huge help :)", "group_id": 1105, "id": 382099}, {"user_id": 4038, "stars": [], "topic_id": 13776, "date_created": 1300458699.331285, "message": "#2 Alka Seltzer + multi-vitamin + water", "group_id": 1105, "id": 382358}, {"user_id": 581, "stars": [], "topic_id": 13776, "date_created": 1300458533.491626, "message": "Wish I had some alka seltzer.", "group_id": 1105, "id": 382319}, {"user_id": 4038, "stars": [], "topic_id": 13776, "date_created": 1300458693.263073, "message": "#1 best cure = hair of the dog", "group_id": 1105, "id": 382356}, {"user_id": 581, "stars": [], "topic_id": 13776, "date_created": 1300458523.5504379, "message": "If I'm clever (rare when drunk) I'll have a pint of water with alka seltzer *before* bed.", "group_id": 1105, "id": 382317}, {"user_id": 3465, "stars": [], "topic_id": 13776, "date_created": 1300460310.9281199, "message": "Don't underestimate the healing power of the McMuffin.", "group_id": 1105, "id": 382588}, {"user_id": 2, "stars": [], "topic_id": 13776, "date_created": 1300481776.7443149, "message": "water, sleep, and some tylenol", "group_id": 1105, "id": 385319}]