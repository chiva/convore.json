[{"user_id": 14243, "stars": [], "topic_id": 15264, "date_created": 1301062780.6882341, "message": "I would like vim to show like textmate the \"\u00ac\" character at the end of each line", "group_id": 100, "id": 438710}, {"user_id": 14243, "stars": [], "topic_id": 15264, "date_created": 1301062796.713726, "message": "also, it would be nice to have some marks to show where tabs are", "group_id": 100, "id": 438716}, {"user_id": 6202, "stars": [], "topic_id": 15264, "date_created": 1301063580.740658, "message": "set listchars=eol:#,tab:>-,trail:#", "group_id": 100, "id": 438823}, {"user_id": 6202, "stars": [], "topic_id": 15264, "date_created": 1301063587.9755249, "message": "set list", "group_id": 100, "id": 438825}, {"user_id": 14243, "stars": [], "topic_id": 15264, "date_created": 1301063746.0821581, "message": "@sumpygump that works! thanks.. although it could be more eyecandy :P", "group_id": 100, "id": 438838}, {"user_id": 14243, "stars": [], "topic_id": 15264, "date_created": 1301063850.7349131, "message": "@sumpygump oh sorry! i thought those where too commands for the same thing.. it works great!", "group_id": 100, "id": 438847}, {"user_id": 2313, "stars": [{"date_created": 1301154376.0745161, "user_id": 1243}, {"date_created": 1301439434.3932991, "user_id": 20745}], "topic_id": 15264, "date_created": 1301064758.1973629, "message": "Vimcasts has an episode on this which explains how to achieve the same look textmate has http://vimcasts.org/episodes/show-invisibles/", "group_id": 100, "id": 439008}, {"user_id": 14243, "stars": [], "topic_id": 15264, "date_created": 1301066755.058269, "message": "@DasIch awesome! that worked", "group_id": 100, "id": 439370}, {"user_id": 14243, "stars": [{"date_created": 1301069440.9073839, "user_id": 9907}], "topic_id": 15264, "date_created": 1301066768.1052649, "message": "i've set it to: :set listchars=eol:\u00ac,tab:\u203a\\ ,trail:\u00ac", "group_id": 100, "id": 439376}]