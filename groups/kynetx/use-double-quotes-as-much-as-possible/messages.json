[{"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306514816.673986, "message": "http:get , http:post", "group_id": 236, "id": 1206850}, {"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306514347.1569121, "message": "datasource calls", "group_id": 236, "id": 1206781}, {"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306514298.3622551, "message": "especially in:", "group_id": 236, "id": 1206772}, {"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306514320.387176, "message": "use javascript resource \"some_url\"", "group_id": 236, "id": 1206777}, {"user_id": 30494, "stars": [{"date_created": 1306517926.1869709, "user_id": 30462}], "topic_id": 37066, "date_created": 1306515058.2530341, "message": "if you need a good reliable cloud-base clock webservice keep using single quotes: \"// KNS Fri May 27 16:31:28 2011\"", "group_id": 236, "id": 1206871}, {"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306514807.9993191, "message": "( http://stackoverflow.com/questions/5684493/using-json-in-krl )", "group_id": 236, "id": 1206849}, {"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306514904.1430011, "message": "mnemotechnic help : \"single quotes wear clown hats\" !", "group_id": 236, "id": 1206859}, {"user_id": 4291, "stars": [], "topic_id": 37066, "date_created": 1306516391.1419461, "message": "@loic LOL!", "group_id": 236, "id": 1207022}, {"user_id": 763, "stars": [], "topic_id": 37066, "date_created": 1306520133.298903, "message": "I'll stick up for single quotes, but only in the context of emitted javascript. :)", "group_id": 236, "id": 1207569}, {"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306735998.6432371, "message": "instead write : {\"key\":\"value\"}", "group_id": 236, "id": 1224648}, {"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306735959.7877109, "message": "don't use single quotes in json string values like this", "group_id": 236, "id": 1224641}, {"user_id": 30494, "stars": [], "topic_id": 37066, "date_created": 1306735978.8769901, "message": "{\"key\":'value'}", "group_id": 236, "id": 1224644}, {"user_id": 30462, "stars": [], "topic_id": 37066, "date_created": 1306756789.4703341, "message": "RFC4627: Strings must be quoted with %x22 <<\">>. Names (\"key\") are strings and therefor must  be quoted.", "group_id": 236, "id": 1226606}, {"user_id": 30462, "stars": [], "topic_id": 37066, "date_created": 1306757060.9911599, "message": "One thing that messed me up: when you look at an object in firebug <<names>> are not quoted. Part of building the JSON representation is quoting the <<names>>.", "group_id": 236, "id": 1226622}]