[{"user_id": 24069, "stars": [], "topic_id": 44615, "date_created": 1314621274.3056309, "message": "Does anyone know how to create and push tags with GitHub for Mac? I tried to create manually with `git tag -a 0.1; git push origin master` but it didn't seem to make it onto GitHub.", "group_id": 591, "id": 1992764}, {"user_id": 27038, "stars": [{"date_created": 1318398295.3796849, "user_id": 15133}], "topic_id": 44615, "date_created": 1314621986.0105729, "message": "Try push --tags", "group_id": 591, "id": 1992836}, {"user_id": 30256, "stars": [{"date_created": 1318398297.0291779, "user_id": 15133}], "topic_id": 44615, "date_created": 1314624933.1303849, "message": "If you want to push only one tag: git push origin 0.1", "group_id": 591, "id": 1993094}, {"user_id": 24069, "stars": [{"date_created": 1314662462.4041741, "user_id": 4149}], "topic_id": 44615, "date_created": 1314654454.0549729, "message": "Awesome, both answers worked. Thanks! Would love to see this added to the Mac client.", "group_id": 591, "id": 1996863}, {"user_id": 24069, "stars": [], "topic_id": 44615, "date_created": 1314654435.515146, "message": "Awesome, both answers worked. Thanks! Would love to see this added to the Mac client.", "group_id": 591, "id": 1996861}, {"user_id": 4149, "stars": [], "topic_id": 44615, "date_created": 1314662500.598757, "message": "Yea tags and multiple remotes should be added it", "group_id": 591, "id": 1997786}, {"user_id": 34773, "stars": [], "topic_id": 44615, "date_created": 1321736098.7508171, "message": "test", "group_id": 591, "id": 2602431}, {"user_id": 34773, "stars": [], "topic_id": 44615, "date_created": 1321736101.5183339, "message": "test", "group_id": 591, "id": 2602432}]